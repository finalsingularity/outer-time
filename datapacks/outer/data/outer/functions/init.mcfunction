scoreboard players set outer.init outer.config 1

# Set default dungeon duration to 10 minutes (600 seconds)
scoreboard players set outer.dungeon.duration outer.config 600

# Remind players of time left in dungeon every 30 seconds
scoreboard players set outer.dungeon.reminder outer.config 30

# Countdown begins at
scoreboard players set outer.dungeon.countdown outer.config 60